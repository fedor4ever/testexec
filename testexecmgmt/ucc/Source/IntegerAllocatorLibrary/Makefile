##########################################################################
#
# Makefile for ...
#
##########################################################################

##########################################################################
#
# Definitions
#
##########################################################################
TARGET=		../../BinInternal/libintalloc.a
INCLUDES=
CFLAGS=		-Wall -gstabs #-ftest-coverage -fprofile-arcs
OBJECTS=	CIntegerAllocator.o
LIBS=

##########################################################################
#
# Rules
#
##########################################################################
$(TARGET):	$(OBJECTS)
	ar -rcs $(TARGET) $(OBJECTS)

%.o:	%.c
	gcc $(CFLAGS) $(INCLUDES) -c $< -o $@

%.o:	%.cpp
	g++ $(CFLAGS) $(INCLUDES) -c $< -o $@

clean:
	rm -r -f $(TARGET) *.o *~ *.bbg *.bb *.gcov *.da 

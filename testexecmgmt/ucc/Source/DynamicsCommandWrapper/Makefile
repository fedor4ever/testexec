##########################################################################
#
# Makefile for ...
#
##########################################################################

##########################################################################
#
# Definitions
#
##########################################################################
TARGET=		../../BinInternal/libdynwrap.a
INCLUDES=
OBJECTS=	CDynamicsCommand.o
LIBS=

##########################################################################
#
# Rules
#
##########################################################################
$(TARGET):	$(OBJECTS) 
	ar -rcs $(TARGET) $(OBJECTS)

%.o:	%.c
	gcc -gstabs -Wall $(INCLUDES) -c $< -o $@

%.o:	%.cpp
	g++ -gstabs -Wall $(INCLUDES) -c $< -o $@

clean:
	rm -r -f $(TARGET) *.o *~

##########################################################################
#
# Makefile for ...
#
##########################################################################

##########################################################################
#
# Definitions
#
##########################################################################
TARGET=		../../BinInternal/libdynamicsconfig.a
INCLUDES=
OBJECTS=	CDynamicsConfigFile.o 	\
		file_utilities.o
LIBS=

##########################################################################
#
# Rules
#
##########################################################################
$(TARGET):	$(OBJECTS) 
	ar -rcs $(TARGET) $(OBJECTS)

%.o:	%.c
	gcc -gstabs -Wall $(INCLUDES) -c $< -o $@

%.o:	%.cpp
	g++ -gstabs -Wall $(INCLUDES) -c $< -o $@

clean:
	rm -r -f $(TARGET) *.o *~

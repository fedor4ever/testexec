###############################################################################
#
# Makefile for 3GNE/BuildTools directory.
#
###############################################################################

###############################################################################
#
# Definitions
#
###############################################################################
include /home/penuser/3GNetworkEmulator/Makefile.conf
TOOLS=	

###############################################################################
#
# Rules
#
###############################################################################
build:
	for dir in ${TOOLS}; do \
		(cd $$dir; ${MAKE} build); \
	done

clean:
	for dir in ${TOOLS}; do \
		(cd $$dir; ${MAKE} clean); \
	done
	rm -r -f *~

